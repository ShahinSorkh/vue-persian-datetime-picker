{
  "name": "vue-persian-datetime-picker",
  "description": "A vue plugin to select jalali date and time",
  "version": "1.2.0",
  "author": "mohammad talkhabi",
  "license": "MIT",
  "main": "dist/vue-persian-datetime-picker.js",
  "keywords": [
    "vuejs",
    "persian",
    "jalali",
    "datepicker",
    "timepicker",
    "datetime picker",
    "vue datepicker",
    "persian datepicker",
    "jalali datepicker",
    "shamsi datepicker"
  ],
  "scripts": {
    "dev": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --hide-modules",
    "export": "./node_modules/.bin/cross-env NODE_ENV=export ./node_modules/.bin/webpack --progress --hide-modules",
    "browser": "./node_modules/.bin/cross-env NODE_ENV=browser ./node_modules/.bin/webpack --progress --hide-modules",
    "buildAll": "yarn build && yarn export && yarn browser"
  },
  "dependencies": {
    "moment-jalaali": "^0.8.3",
    "vue-awesome": "^3.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talkhabi/vue-persian-datetime-picker"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "bootstrap": "^4.1.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "highlight.js": "^9.12.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue": "^2.4.4",
    "vue-highlight.js": "^2.2.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
